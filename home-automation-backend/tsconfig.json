{
    "compilerOptions": {
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "paths": {
        "node-rtsp-stream": ["./types/node-rtsp-stream"]
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "src/generated"]
  }
  